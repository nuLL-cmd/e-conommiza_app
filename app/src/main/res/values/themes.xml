<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Econommiza_app" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/blue_545de8</item>
        <item name="colorPrimaryVariant">@color/background_form</item>
        <item name="colorOnPrimary">@color/white_fff</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/blue_545de8</item>
        <item name="colorSecondaryVariant">@color/blue_545de8</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <item name="android:windowLightStatusBar">true</item>

        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.Econommiza_app_two" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/blue_545de8</item>
        <item name="colorPrimaryVariant">@color/button_positive</item>
        <item name="colorOnPrimary">@color/white_fff</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/blue_545de8</item>
        <item name="colorSecondaryVariant">@color/blue_545de8</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>

        <!-- Customize your theme here. -->
    </style>


    <style name="DialogTheme" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorAccent">@color/button_positive</item>
    </style>
</resources>